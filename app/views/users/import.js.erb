var skillList = $('#skill-list');
skillList.html("");
<% @user_profile.skills.each do |skill| %>
	setTimeout(fadeSkill("<%= skill %>", nextSkillId()), 0);
<% end %>
$('#user_first_name').val("<%= j(@user_profile.first_name) %>");
$('#user_last_name').val("<%= j(@user_profile.last_name) %>");
$('#user_location').val("<%= j(@user_profile.location) %>");
$('#user_bio').text("<%= j(@user_profile.summary) %>");
$('#user_email').focus();